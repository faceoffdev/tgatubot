{
    "name": "faceoff/tgatubot",
    "type": "project",
    "description": "Telegram tgatu bot",
    "keywords": [
        "telegram",
        "bot",
        "tgatubot"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/faceoffdev/driver-telegram"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-mbstring": "*",
        "botman/botman": "^2.6",
        "botman/driver-telegram": "dev-master",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.4",
        "laravel/framework": "^8.89",
        "netresearch/jsonmapper": "^4.0",
        "sentry/sentry-laravel": "^2.11",
        "staudenmeir/eloquent-json-relations": "^1.5",
        "thecodingmachine/discovery": "^1.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "facade/ignition": "^2.17",
        "friendsofphp/php-cs-fixer": "^3.5",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^5.11",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "php-cs-fixer": "php-cs-fixer fix --diff --config=.php-cs-fixer.dist.php",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
